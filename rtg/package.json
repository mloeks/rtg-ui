{
  "name": "rtg",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "blob-polyfill": "^2.0.20171115",
    "canvas-toBlob": "^1.0.0",
    "date-fns": "^1.29.0",
    "eslint-config-airbnb": "^16.1.0",
    "hsl-to-hex": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "material-ui": "^0.20.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-avatar-editor": "^11.0.3",
    "react-dom": "^16.3.2",
    "react-minimal-pie-chart": "^3.0.2",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "^1.0.3",
    "react-scripts": "^1.1.4",
    "react-tap-event-plugin": "^3.0.2",
    "stickybits": "^3.3.2",
    "verge": "^1.10.2",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "REACT_APP_ENV=production npm-run-all build-css build-js",
    "build:demo": "REACT_APP_ENV=demo npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
