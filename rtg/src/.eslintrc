{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "document": true,
    "fetch": true,
    "Element": true
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to", "hrefLeft", "hrefRight" ],
      "aspects": [ "noHref", "invalidHref", "preferButton" ]
    }],
    "no-underscore-dangle": [ "error", {
      "allowAfterThis": true
    }]
  }
}